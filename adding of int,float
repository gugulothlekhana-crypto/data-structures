#include<iostream>
using namespace std;
class math{
    public:
    int add(int a, int b){
        return a+b;
    }
    float add(float a, float b){
        return a+b;
    }
    int add(int a, int b, intc){
        return a+b+c;
    }
    };
    int main(){
        math m;
        cout << "add two numbers" << m.add(15,25) << endl;
        cout << "add two floats" << m.add(1.5f,3.5f) << endl;
        cout << "add three integers" << m.add(5,4,3) << endl;
        return 0;
    }

